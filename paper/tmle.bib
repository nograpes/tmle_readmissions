	urldate = {2012-02-06},
	journal = {Academic Emergency Medicine: Official Journal of the Society for Academic Emergency Medicine},
	author = {Mazer, Maryann and Deroos, Francis and Hollander, Judd E and {McCusker}, Christine and Peacock, Nicholas and Perrone, Jeanmarie},
