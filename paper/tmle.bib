	keywords = {Adolescent, Adult, Aged, Aged, 80 and over, Child, Child, Preschool, Feasibility Studies, Female, Health Services Research, Humans, Infant, London, Male, Middle Aged, Outcome and Process Assessment (Health Care), Patient Readmission, Quality of Health Care, Retrospective Studies},
	pages = {1134--1136}
