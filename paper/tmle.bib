	urldate = {2011-12-11},
	journal = {Circulation. Cardiovascular Quality and Outcomes},
	author = {Keenan, Patricia S and Normand, Sharon-Lise T and Lin, Zhenqiu and Drye, Elizabeth E and Bhat, Kanchana R and Ross, Joseph S and Schuur, Jeremiah D and Stauffer, Brett D and Bernheim, Susannah M and Epstein, Andrew J and Wang, Yongfei and Herrin, Jeph and Chen, Jersey and Federer, Jessica J and Mattera, Jennifer A and Wang, Yun and Krumholz, Harlan M},
	month = sep,
	year = {2008},
	note = {{PMID:} 20031785},
