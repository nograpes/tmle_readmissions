	keywords = {Adult, Confounding Factors (Epidemiology), Hospital Mortality, Hospitals, Humans, Models, Theoretical, Outcome and Process Assessment (Health Care), Patient Readmission, Quality of Health Care, Reproducibility of Results, United States},
	pages = {1533--1541}
}

