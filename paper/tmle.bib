	keywords = {Diagnostic Errors, Drug Therapy, Female, Humans, Iatrogenic Disease, Incidence, Interviews as Topic, Laboratories, Hospital, Male, Medical Audit, Medical Errors, Medical Records, Middle Aged, Outcome Assessment (Health Care), Patient Discharge, Prospective Studies},
