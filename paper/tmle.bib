	keywords = {Abstracting and Indexing as Topic, Health Services Research, Hospital Mortality, Hospitals, Humans, Logistic Models, Outcome Assessment (Health Care), Patient Readmission, Postoperative Complications, Regression Analysis, Risk Factors, United States},
	pages = {425--445}
