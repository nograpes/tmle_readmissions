@article{van_walraven_incidence_2011,
	title = {Incidence of potentially avoidable urgent readmissions and their relation to all-cause urgent readmissions},
	volume = {183},
	issn = {1488-2329},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/21859870},
	doi = {10.1503/cmaj.110400},
	abstract = {{BACKGROUND}

Urgent, unplanned hospital readmissions are increasingly being used to gauge the quality of care. We reviewed urgent readmissions to determine which were potentially avoidable and compared rates of all-cause and avoidable readmissions.


{METHODS}

In a multicentre, prospective cohort study, we reviewed all urgent readmissions that occurred within six months among patients discharged to the community from 11 teaching and community hospitals between October 2002 and July 2006. Summaries of the readmissions were reviewed by at least four practising physicians using standardized methods to judge whether the readmission was an adverse event (poor clinical outcome due to medical care) and whether the adverse event could have been avoided. We used a latent class model to determine whether the probability that each readmission was truly avoidable exceeded 50\%.


{RESULTS}

Of the 4812 patients included in the study, 649 (13.5\%, 95\% confidence interval [{CI]} 12.5\%-14.5\%) had an urgent readmission within six months after discharge. We considered 104 of them (16.0\% of those readmitted, 95\% {CI} 13.3\%-19.1\%; 2.2\% of those discharged, 95\% {CI} 1.8\%-2.6\%) to have had a potentially avoidable readmission. The proportion of patients who had an urgent readmission varied significantly by hospital (range 7.5\%-22.5\%; χ(2) = 92.9, p {\textless} 0.001); the proportion of readmissions deemed avoidable did not show significant variation by hospital (range 1.2\%-3.7\%; χ(2) = 12.5, p {\textless} 0.25). We found no association between the proportion of patients who had an urgent readmission and the proportion of patients who had an avoidable readmission (Pearson correlation 0.294; p = 0.38). In addition, we found no association between hospital rankings by proportion of patients readmitted and rankings by proportion of patients with an avoidable readmission (Spearman correlation coefficient 0.28, p = 0.41).


{INTERPRETATION}

Urgent readmissions deemed potentially avoidable were relatively uncommon, comprising less than 20\% of all urgent readmissions following hospital discharge. Hospital-specific proportions of patients who were readmitted were not related to proportions with a potentially avoidable readmission.},
	number = {14},
	urldate = {2011-12-05},
	journal = {{CMAJ:} Canadian Medical Association Journal = Journal De {l'Association} Medicale Canadienne},
	author = {van Walraven, Carl and Jennings, Alison and Taljaard, Monica and Dhalla, Irfan and English, Shane and Mulpuru, Sunita and Blecker, Saul and Forster, Alan J},
	month = oct,
	year = {2011},
	note = {{PMID:} 21859870},
	pages = {E1067--1072},
	annote = {An interesting study, which contradicts a fundamental assumption of my entire line of research. The study question is good, is the proportion of avoidable readmissions related to readmissions? If it is, then readmissions are a good proxy for avoidable readmissions, which are a good proxy for quality of care.
He follows a cohort in Ottawa of 4812 over 11 hospitals. 600 get readmitted in six months (which itself is suspiciously low). He gets four physicians to classify them as avoidable and resolves disagreement using some latent class analysis. 102 are avoidable.
He then looks at the proportion readmitted, and ranks. Then he looks at the proportion avoidably readmitted, and ranks. He uses Spearman's to correlate, and finds no evidence for correlation.
Critically, he did not adjust the readmission rates for patient risk. So he is just relating crude readmission risk, which nobody claims is a good proxy for avoidable readmission risk. I see why he didn't; there would be not enough power.
Indeed with only 6 - 12 avoidable cases per hospital, the proportion could be strongly swayed by a few cases. Furthermore, since the rankings were close, then the ranking could be very strongly affected by slight sampling error. I could probably show this conclusively, but I think it is rather obvious.
I should try and be polite about it though.
 }
}

@article{kik_tuberculosis_2008,
	title = {Tuberculosis outbreaks predicted by characteristics of first patients in a {DNA} fingerprint cluster},
	volume = {178},
	issn = {1535-4970},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/18388352},
	doi = {10.1164/rccm.200708-1256OC},
	abstract = {{RATIONALE}

Some clusters of patients who have Mycobacterium tuberculosis isolates with identical {DNA} fingerprint patterns grow faster than others. It is unclear what predictors determine cluster growth.


{OBJECTIVES}

To assess whether the development of a tuberculosis ({TB)} outbreak can be predicted by the characteristics of its first two patients.


{METHODS}

Demographic and clinical data of all culture-confirmed patients with {TB} in the Netherlands from 1993 through 2004 were combined with {DNA} fingerprint data. Clusters were restricted to cluster episodes of 2 years to only detect newly arising clusters. Characteristics of the first two patients were compared between small (2-4 cases) and large (5 or more cases) cluster episodes. Measurements and Main Results: Of 5,454 clustered cases, 1,756 (32\%) were part of a cluster episode of 2 years. Of 622 cluster episodes, 54 (9\%) were large and 568 (91\%) were small episodes. Independent predictors for large cluster episodes were as follows: less than 3 months' time between the diagnosis of the first two patients, one or both patients were young ({\textless}35 yr), both patients lived in an urban area, and both patients came from sub-Saharan Africa.


{CONCLUSIONS}

In the Netherlands, patients in new cluster episodes should be screened for these risk factors. When the risk pattern applies, targeted interventions (e.g., intensified contact investigation) should be considered to prevent further cluster expansion.},
	number = {1},
	urldate = {2012-01-04},
	journal = {American Journal of Respiratory and Critical Care Medicine},
	author = {Kik, Sandra V and Verver, Suzanne and van Soolingen, Dick and de Haas, Petra E W and Cobelens, Frank G and Kremer, Kristin and van Deutekom, Henk and Borgdorff, Martien W},
	month = jul,
	year = {2008},
	note = {{PMID:} 18388352},
	keywords = {Adolescent, Adult, Child, Child, Preschool, Cluster Analysis, Disease Outbreaks, {DNA} Fingerprinting, {DNA}, Bacterial, Female, Humans, Infant, Male, Molecular Epidemiology, Mycobacterium tuberculosis, Netherlands, Tuberculosis},
	pages = {96--104}
}

